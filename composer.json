{
  "name": "nepster-web/simple-queue",
  "description": "Simple queues implementation in PHP through database.",
  "keywords": [
    "php",
    "queue",
    "simple-queue",
    "dbal queue",
    "database queue",
    "consumer"
  ],
  "support": {
    "docs": "https://github.com/nepster-web/simple-queue/blob/main/docs/guide/README.md",
    "issues": "https://github.com/nepster-web/simple-queue/issues",
    "source": "https://github.com/nepster-web/simple-queue"
  },
  "config": {
    "sort-packages": true
  },
  "license": "MIT",
  "authors": [
  ],
  "require": {
    "php": "~7.4 || ^8.0",
    "ext-PDO": "*",
    "ext-json": "*",
    "doctrine/dbal": "^3.0",
    "ramsey/uuid": "^4.1",
    "laminas/laminas-hydrator": "^4.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Simple\\Queue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Simple\\QueueTest\\": "tests/"
    }
  }
}